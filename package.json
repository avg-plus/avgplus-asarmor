{
  "name": "asarmor",
  "version": "2.0.0",
  "description": "Protect asar archive file from extraction",
  "main": "build/src/index.js",
  "scripts": {
    "test": "jest",
    "start": "node ./build/bin/asarmor.js",
    "clean": "rimraf build",
    "prepare": "npm run build && node ./scripts/keygen.js && node ./scripts/js2c.js && npm run gyp",
    "gyp:config": "node ./node_modules/node-gyp/bin/node-gyp.js configure --target=16.0.2 --disturl=https://electronjs.org/headers",
    "gyp:build": "node ./node_modules/node-gyp/bin/node-gyp.js build",
    "gyp:build:debug": "node ./node_modules/node-gyp/bin/node-gyp.js build --debug",
    "gyp": "npm run gyp:config && npm run gyp:build",
    "keygen": "node ./scripts/keygen.js",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "prepack": "npm run clean && npm run build",
    "prettier": "npm run prettier --write src/**/*.ts"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "example"
    ]
  },
  "author": "Sleeyax",
  "license": "MIT",
  "dependencies": {
    "asar": "^3.1.0",
    "chromium-pickle-js": "^0.2.0",
    "commander": "^6.1.0"
  },
  "bin": "./build/bin/asarmor.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sleeyax/asarmor"
  },
  "keywords": [
    "asar",
    "asar protection",
    "asar patch",
    "asar anti-unpacking",
    "asar anti-extraction",
    "electron asar"
  ],
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^14.6.4",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "babel-jest": "^27.3.1",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^27.3.1",
    "node-addon-api": "^5.0.0",
    "node-gyp": "^9.0.0",
    "prettier": "2.6.2",
    "rimraf": "^3.0.2",
    "terser": "^5.13.1",
    "typescript": "^4.0.2"
  }
}
